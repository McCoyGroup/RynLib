
# Since we're updating, we build off the existing image

#FROM rynimg-centos
FROM 5b3ac9909805

RUN rm -rf /home/RynLib
ADD . /home/RynLib
RUN mkdir -p /tests

RUN python3 /home/RynLib/CLI.py config build-libs --rebuild=True
RUN python3 /home/RynLib/CLI.py config run-tests --debug=True

WORKDIR /home